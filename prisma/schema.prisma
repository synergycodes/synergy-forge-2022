// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Integration {
  id                Int             @id @default(autoincrement())
  name              String
  budget            Int
  participants      Int
  date_start        DateTime
  date_end          DateTime
}
